{
  "tasks": [
    {
      "type": "cppbuild",
      "label": "clang build rkutil",
      "command": "/usr/bin/clang",
      "args": [
        "-fdiagnostics-color=always",
        "-I${headers}",
        "-I${HOMEBREW_PREFIX}/include",
        "-I${HOMEBREW_PREFIX}/opt/openssl@1.1/include",
        "-g",
        "rkutil.c",
        "-o",
        "${fileDirname}/rkutil",
        "-L.",
        "-L${HOMEBREW_PREFIX}/lib",
        "-L${HOMEBREW_PREFIX}/opt/openssl@1.1/lib",
        "-lradarkit",
        "-lfftw3f",
        "-lnetcdf",
        "-lssl"
      ],
      "options": {
        "cwd": "${fileDirname}"
      },
      "problemMatcher": ["$gcc"],
      "group": {
        "kind": "build",
        "isDefault": true
      },
      "detail": "Task generated by Debugger and modified by hand."
    }
  ],
  "version": "2.0.0"
}
