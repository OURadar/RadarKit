{
  "configurations": [
    {
      "name": "macos-clang-arm64",
      "includePath": [
        "${workspaceFolder}/**",
        "${workspaceFolder}/headers/RadarKit",
        "${HOMEBREW_PREFIX}/include",
        "${HOMEBREW_PREFIX}/opt/openssl@1.1/include"
      ],
      "defines": [],
      "macFrameworkPath": [
        "/Library/Developer/CommandLineTools/SDKs/MacOSX.sdk/System/Library/Frameworks"
      ],
      "compilerPath": "/usr/bin/clang",
      "cStandard": "${default}",
      "cppStandard": "${default}",
      "intelliSenseMode": "macos-clang-arm64"
    },
    {
      "name": "linux-gcc-x86_64",
      "includePath": [
        "${workspaceFolder}/headers",
        "${workspaceFolder}/headers/RadarKit",
        "/usr/include",
        "/usr/local/include"
      ],
      "defines": [],
      "compilerPath": "/usr/bin/gcc",
      "cStandard": "c17",
      "cppStandard": "c++14",
      "intelliSenseMode": "linux-clang-x64"
    },
    {
      "name": "linux-gcc-x86_64-avx512",
      "includePath": [
        "${workspaceFolder}/headers",
        "${workspaceFolder}/headers/RadarKit",
        "/usr/include",
        "/usr/local/include"
      ],
      "defines": [
        "__AVX512F__",
        "__FMA__"
      ],
      "compilerPath": "/usr/bin/gcc",
      "cStandard": "c17",
      "cppStandard": "c++14",
      "intelliSenseMode": "linux-clang-x64"
    }
  ],
  "version": 4
}